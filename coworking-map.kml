---
title : Coworking Map (KML)
---
<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.2">
  <Document>
    <name>Coworking Map</name>

    <Style id="todo_coworking_space">
      <IconStyle>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/paddle/wht-blank.png</href>
        </Icon>
      </IconStyle>
    </Style>

    <Style id="todo_coffee_shop">
      <IconStyle>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/paddle/wht-blank.png</href>
        </Icon>
      </IconStyle>
    </Style>

    <Style id="todo_city">
      <IconStyle>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/paddle/wht-blank.png</href>
        </Icon>
      </IconStyle>
    </Style>

    {% for place in site.coworking_spaces %}
      <Placemark id="{{ place.filename }}">
        <name>{{ place.title }}</name>
        <styleUrl>#{{ place.type }}</styleUrl>
        <description>
          <![CDATA[
            {{ place.content }}
          ]]>
        </description>
        <Point>
          <coordinates>{{ place.coordinates }}</coordinates>
        </Point>
      </Placemark>
    {% endfor %}
  </Document>
</kml>
